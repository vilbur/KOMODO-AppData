/*  roundFloat 123.456 -2 >>> 100.0
	roundFloat 123.456  0 >>> 123.0
	roundFloat 123.456  2 >>> 123.46
*/
fn roundFloat val decimal_palces = ( local mult = 10.0 ^ decimal_palces; (floor ((val * mult) + 0.5)) / mult )!@#_currentPos!@#_anchor