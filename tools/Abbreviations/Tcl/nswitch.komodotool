{
  "keyboard_shortcut": "", 
  "name": "nswitch", 
  "language": "Tcl", 
  "value": [
    "<% // Tcl dynamic switch-snippet", 
    "var i, numReps = ko.snippets.consumeLeadingNumericFactor(\":\");", 
    "%>", 
    "switch [[%tabstop:STRING]] {", 
    "<% for (i = 0; i < numReps; i++) { %>", 
    "\t[[%tabstop:PATTERN<%= i %>]] {", 
    "\t\t[[%tabstop:ACTION<%= i %>]]", 
    "\t}", 
    "<% } %>", 
    "}[[%tabstop:]]", 
    "!@#_currentPos!@#_anchor"
  ], 
  "set_selection": "false", 
  "version": "1.1.5", 
  "treat_as_ejs": "true", 
  "type": "snippet", 
  "indent_relative": "true", 
  "auto_abbreviation": "true"
}