  "value": [
    "[[%tabstop:function ObjClone(original){", 
    "    var ObjClone = Object.create( Object.getPrototypeOf( original ) ) ;", 
    "    var i , keys = Object.getOwnPropertyNames( original ) ;", 
    "    for ( i = 0 ; i < keys.length ; i ++ ){", 
    "        Object.defineProperty( ObjClone , keys[ i ] ,", 
    "            Object.getOwnPropertyDescriptor( original , keys[ i ] )", 
    "        ) ;", 
    "    }", 
    "    return ObjClone ;", 
    "}]]", 
