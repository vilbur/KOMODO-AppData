[[%tabstop://[VarSet, $ZFILE_UTILS_DLL, "ZFileUtils64.dll"]]]
[VarSet, file_path, [FileNameResolvePath, [StrMerge, [[%tabstop:Subfolder]], "\[[%tabstop:file]]"] ]]
[FileExecute, $ZFILE_UTILS_DLL, "LaunchAppWithFile",  #file_path]!@#_currentPos!@#_anchor